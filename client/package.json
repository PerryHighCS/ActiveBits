{
  "name": "activebits-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "test": "npm run test:unit",
    "test:unit": "sh -c 'set -e; files=$(find src \\( -name \"*.test.js\" -o -name \"*.test.ts\" -o -name \"*.test.tsx\" \\) -print); if [ -z \"$files\" ]; then echo \"No client tests found\"; exit 0; fi; node --import tsx --test $files'",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.3",
    "qrcode.react": "^4.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.12.0",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.3",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/node": "^25.2.1",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@vitejs/plugin-react": "^5.1.0",
    "eslint": "^9.21.0",
    "eslint-plugin-jsdoc": "^62.5.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.2"
  }
}
