{
  "name": "activebits-server",
  "version": "1.0.0",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js --trace-warnings",
    "build": "tsc -p tsconfig.build.json",
    "test": "sh -c 'set -e; files=$(find . -not -path \"./node_modules/*\" \\( -name \"*.test.js\" -o -name \"*.test.ts\" \\) -print); if [ -z \"$files\" ]; then echo \"No server tests found\"; exit 0; fi; node --test $files'",
    "typecheck": "tsc --noEmit",
    "test:valkey": "node test-valkey.js",
    "monitor:valkey": "node monitor-valkey.js"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "description": "",
  "dependencies": {
    "cookie-parser": "^1.4.7",
    "express": "^5.2.0",
    "http-proxy-middleware": "^3.0.5",
    "ioredis": "^5.8.2",
    "ws": "^8.18.3"
  }
}
