{
  "name": "activebits-server",
  "version": "1.0.0",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "start": "sh -c 'if [ -f dist/server.js ]; then node dist/server.js; else node --import tsx server.ts; fi'",
    "dev": "node --trace-warnings --import tsx server.ts",
    "build": "tsc -p tsconfig.build.json",
    "test": "sh -c 'set -e; files=$(find . -not -path \"./node_modules/*\" \\( -name \"*.test.js\" -o -name \"*.test.ts\" \\) -print); if [ -z \"$files\" ]; then echo \"No server tests found\"; exit 0; fi; node --import tsx --test $files'",
    "typecheck": "tsc --noEmit",
    "test:valkey": "node --import tsx test-valkey.ts",
    "monitor:valkey": "node --import tsx monitor-valkey.ts"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "description": "",
  "dependencies": {
    "cookie-parser": "^1.4.7",
    "express": "^5.2.0",
    "http-proxy-middleware": "^3.0.5",
    "ioredis": "^5.8.2",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.3",
    "@types/node": "^25.2.1",
    "@types/ws": "^8.18.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
